{
  "name": "hyde",
  "version": "0.0.1",
  "description": "a content management system for Jekyll-based websites",
  "private": true,
  "engines": {
    "node": "7.x"
  },
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "axios": "^0.16.1",
    "body-parser": "^1.17.1",
    "child-process-promise": "^2.2.1",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "fs": "0.0.1-security",
    "interweave": "^4.1.0",
    "marked": "^0.3.6",
    "morgan": "^1.8.1",
    "node-dir": "^0.1.16",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-scripts": "^0.9.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.7.1",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.1.0",
    "sinon-stub-promise": "^4.0.0"
  },
  "scripts": {
    "dev": "concurrently  \"yarn react\" \"yarn start-server\"",
    "prestart": "yarn build",
    "start": "yarn start-server",
    "start-server": "babel-node server --presets es2015,stage-2",
    "react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isaacrowntree/hyde"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
