{
  "name": "hyde",
  "version": "0.0.1",
  "description": "a content management system for Jekyll-based websites",
  "private": true,
  "engines": {
    "node": "8.x"
  },
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.17.1",
    "child-process-promise": "^2.2.1",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "fs": "0.0.1-security",
    "interweave": "^8.0.2",
    "marked": "^0.3.6",
    "morgan": "^1.8.1",
    "node-dir": "^0.1.16",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-scripts": "1.0.17"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^16.2.0",
    "sinon": "^4.1.3",
    "sinon-stub-promise": "^4.0.0"
  },
  "scripts": {
    "dev": "concurrently  \"yarn react\" \"yarn dev-server\"",
    "dev-server": "nodemon --watch server --exec babel-node server --presets es2015,stage-2",
    "prestart": "yarn build",
    "start": "yarn server",
    "server": "babel-node server --presets es2015,stage-2",
    "react": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isaacrowntree/hyde"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
