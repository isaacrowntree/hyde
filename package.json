{
  "name": "hyde",
  "version": "0.0.1",
  "description": "a content management system for Jekyll-based websites",
  "engines": {
    "node": "7.8.0"
  },
  "private": true,
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "body-parser": "^1.17.1",
    "concurrently": "^3.1.0",
    "express": "^4.15.2",
    "fs": "0.0.1-security"
  },
  "scripts": {
    "prestart": "cd app && npm install && cd ..",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "babel-node server/server.js",
    "client": "babel-node app/start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint .",
    "test": "./node_modules/.bin/mocha --reporter spec server/test"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "3.1.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "mocha": "^3.2.0",
    "request": "^2.81.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isaacrowntree/hyde"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
